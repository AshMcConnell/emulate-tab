{
  "name": "emulate-tab",
  "version": "1.0.0",
  "description": "emulate tab key without selector or dependencies",
  "main": "dist/index.js",
  "scripts": {
    "test": "node test.js",
    "build": "tsc",
    "build:amd": "tsc -p tsconfig.amd.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "prebuild:all": "rm -rf dist",
    "build:all": "npm run build",
    "postbuild:all": "npm run build:all:amd",
    "build:all:amd": "npm run build:amd",
    "postbuild:all:amd": "npm run build:all:umd",
    "build:all:umd": "npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-hein/emulate-tab.git"
  },
  "keywords": [
    "emulate",
    "tab"
  ],
  "author": "Emanuel Hein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e-hein/emulate-tab/issues"
  },
  "homepage": "https://github.com/e-hein/emulate-tab#readme",
  "devDependencies": {
    "shelljs": "^0.8.4",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist"
  ]
}
